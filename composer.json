{
    "name": "arraytics/tools",
    "description": "A PHP package for tools.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Arraytics\\Tools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arraytics\\Tools\\Tests\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
